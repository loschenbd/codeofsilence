      "type": "rich-text",
      "blocks": {
        "1661611103082e236b-0": {
          "type": "heading",
          "settings": {
            "title": "Neutral-LR™ Technology"
          }
        }
      },
      "block_order": [
        "1661611103082e236b-0"
